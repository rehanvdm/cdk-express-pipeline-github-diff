{
  "name": "cdk-express-pipeline-github-diff",
  "description": "GitHub Actions TypeScript template",
  "version": "0.0.1",
  "author": "",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/rehanvdm/cdk-express-pipeline-github-diff",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rehanvdm/cdk-express-pipeline-github-diff.git"
  },
  "bugs": {
    "url": "https://github.com/rehanvdm/cdk-express-pipeline-github-diff/issues"
  },
  "keywords": [
    "actions"
  ],
  "exports": {
    ".": "./dist/index.cjs"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "npx jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "local-action": "npx @github/local-action . src/main/main.ts .env",
    "package": "node build.js",
    "package:watch": "node build.js --watch",
    "test": "npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^4.0.5",
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@aws-cdk/toolkit-lib": "^1.4.0",
    "@octokit/core": "^7.0.3",
    "@octokit/plugin-rest-endpoint-methods": "^16.0.0",
    "@octokit/webhooks-definitions": "^3.68.1",
    "@types/js-yaml": "^4.0.9",
    "aws-cdk-lib": "^2.212.0",
    "cdk-express-pipeline": "^1.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@github/local-action": "^5.1.0",
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.32.1",
    "esbuild": "^0.20.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "ts-jest": "^29.4.0",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.8.3"
  }
}
